    if AimLockEnabled then
        if not AimTarget or not AimTarget.Character or not AimTarget.Character:FindFirstChild(AimPart) or AimTarget.Character.Humanoid.Health <= 0 then
            AimTarget = getClosestToCrosshair()
        end
        if AimTarget and AimTarget.Character and AimTarget.Character:FindFirstChild(AimPart) then
            local pos = AimTarget.Character[AimPart].Position
            Camera.CFrame = CFrame.new(Camera.CFrame.Position, pos)
        end
    end
end)

    end,
 })

