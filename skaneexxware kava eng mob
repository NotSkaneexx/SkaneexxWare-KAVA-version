local startCFrame = hrp.CFrame

local function tpTo(pos)
    hrp.CFrame = CFrame.new(pos)
end

local function triggerPrompt(prompt)
    if not prompt then return false end
    if typeof(fireproximityprompt) == "function" then
        fireproximityprompt(prompt)
        return true
    elseif typeof(firesignal) == "function" then
        pcall(function() firesignal(prompt.Triggered) end)
        return true
    elseif prompt.InputHoldBegin then
        prompt:InputHoldBegin()
        task.wait(0.1)
        prompt:InputHoldEnd()
        return true
    end
    return false
end

local model = workspace:FindFirstChild("for_pickup") and workspace.for_pickup:FindFirstChild("FakeKeycard")
local handle = model and model:FindFirstChild("Handle")
local prompt = nil
if handle then
    for _, v in ipairs(handle:GetDescendants()) do
        if v:IsA("ProximityPrompt") then
            prompt = v
            break
        end
    end
end

tpTo(startCFrame.Position + Vector3.new(0,2000,0))
task.wait(0.1)

tpTo(Vector3.new(-17,2000,100))
task.wait(0.1)

tpTo(Vector3.new(-17,19,100))
task.wait(0.5)

if prompt then
    triggerPrompt(prompt)
end
task.wait(0.1)

tpTo(Vector3.new(-16,19,78))
task.wait(0.1)

tpTo(Vector3.new(-7,20,68))
task.wait(0.6)

tpTo(Vector3.new(-7,2000,68))
task.wait(0.5)

tpTo(startCFrame.Position + Vector3.new(0,2000,0))
task.wait(0.3)

tpTo(startCFrame.Position)

    end,
 })

