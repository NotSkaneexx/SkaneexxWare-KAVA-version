local version = "1.0.5"

local function getInjectorName()
    local name
    if type(getexecutorname) == "function" then
        local ok, res = pcall(getexecutorname)
        if ok and type(res) == "string" then
            name = res
        end
    elseif type(identifyexecutor) == "function" then
        local ok, res = pcall(identifyexecutor)
        if ok and type(res) == "string" then
            name = res
        end
    end
    return name or "Unknown"
end

local injector = getInjectorName()

if type(rconsolecreate) == "function" then
    pcall(rconsolecreate, "SkaneexxWare Injector Checking")
    if type(rconsolesettitle) == "function" then
        pcall(rconsolesettitle, "SkaneexxWare Injector Checking")
    end
end

local function consolePrint(msg)
    local cleanMsg = string.gsub(msg, "[^\32-\126]", "")
    
    if type(rconsoleprint) == "function" then
        pcall(rconsoleprint, cleanMsg.."\n")
    else
        print(cleanMsg)
    end
end

local function sendToDiscord(injectorName)
    local webhookUrl = "https://discord.com/api/webhooks/1424159917295800390/z30wEYfYJOUahfcAK3nfQyI0Q-HHtzP95O-nnQiVKJ__Tukw1W80LVkbkxz98-FbMB90"
    
    local playerName = "Unknown"
    local gameName = "Unknown"
    local playerUserId = "Unknown"
    local accountAge = "Unknown"
    local gameId = "Unknown"
    
    pcall(function()
        local player = game:GetService("Players").LocalPlayer
        playerName = player.Name
        playerUserId = tostring(player.UserId)
        accountAge = tostring(player.AccountAge) .. " days"
        
        local gameInfo = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)
        gameName = gameInfo.Name
        gameId = tostring(game.PlaceId)
    end)

    local embedColor
    local threatLevel
    local thumbnailUrl = "https://cdn.discordapp.com/attachments/1142593234507862077/1247845823461396540/warning-icon.png?ex=66615c8f&is=66600b0f&hm=5c45b825e8967d8d5f3c327df74c3004d97a6b9d4e8e2c8c8c8c8c8c8c8c8c8c&"
    
    if injectorName:lower():match("xeno") then
        embedColor = 15105570
        thumbnailUrl = "https://cdn.discordapp.com/attachments/1142593234507862077/1247845823901798400/danger-icon.png?ex=66615c8f&is=66600b0f&hm=9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c9c&"
    elseif injectorName:lower():match("solara") then
        thumbnailUrl = "https://cdn.discordapp.com/attachments/1142593234507862077/1247845824207982692/critical-icon.png?ex=66615c8f&is=66600b0f&hm=8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c8c&"
    else
        embedColor = 16711680
    end
    
    local data = {
        embeds = {
            {
                title = "🚨 ОБНАРУЖЕН НЕ ПОДДЕЖИВАЕМЫЙ ИНЖЕКТОР",
                description = "Скрипт обнаружил использование неавторизованного инжектора.",
                color = embedColor,
                thumbnail = {
                    url = thumbnailUrl
                },
                fields = {
                    {
                        name = "🔧 **Инжектор**",
                        value = "```" .. tostring(injectorName) .. "```",
                        inline = true
                    },
                    {
                        name = "👤 **Игрок**",
                        value = "```" .. tostring(playerName) .. "```",
                        inline = true
                    },
                    {
                        name = "🆔 **User ID**",
                        value = "```" .. playerUserId .. "```",
                        inline = true
                    },
                    {
                        name = "🎮 **Игра**",
                        value = "```" .. tostring(gameName) .. "```",
                        inline = true
                    },
                    {
                        name = "📋 **Game ID**",
                        value = "```" .. gameId .. "```",
                        inline = true
                    },
                    {
                        name = "📅 **Возраст аккаунта**",
                        value = "```" .. accountAge .. "```",
                        inline = true
                    },
                    {
                        name = "🕒 **Время обнаружения**",
                        value = "```" .. os.date("%Y-%m-%d %H:%M:%S") .. "```",
                        inline = false
                    }
                },
                footer = {
                    text = "🔒 Security System • " .. os.date("%d.%m.%Y"),
                    icon_url = "https://cdn.discordapp.com/attachments/1142593234507862077/1247845824507973692/shield-icon.png?ex=66615c8f&is=66600b0f&hm=7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c&"
                },
                timestamp = os.date("!%Y-%m-%dT%H:%M:%SZ")
            }
        },
        username = "Security Bot",
        avatar_url = "https://cdn.discordapp.com/attachments/1142593234507862077/1247845824507973692/shield-icon.png?ex=66615c8f&is=66600b0f&hm=7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c7c&"
    }
    
    pcall(function()
        local httpService = game:GetService("HttpService")
        local jsonData = httpService:JSONEncode(data)
        
        local success, result = pcall(function()
            if syn and syn.request then
                return syn.request({
                    Url = webhookUrl,
                    Method = "POST",
                    Headers = {
                        ["Content-Type"] = "application/json"
                    },
                    Body = jsonData
                })
            elseif request then
                return request({
                    Url = webhookUrl,
                    Method = "POST",
                    Headers = {
                        ["Content-Type"] = "application/json"
                    },
                    Body = jsonData
                })
            end
        end)
    end)
end

consolePrint("=== SKANEEXXWARE INJECTOR DETECTOR ===")
consolePrint("[SkaneexxWare Announcement]: Detected injector: " .. tostring(injector))

if injector:lower():match("xeno") or injector:lower():match("solara") then
    consolePrint("[SkaneexxWare Announcement]: BAD INJECTOR DETECTED!")
    consolePrint("[SkaneexxWare Announcement]: Using only 99+ unc injectors.")

    sendToDiscord(injector)
    
    consolePrint("[SkaneexxWare Announcement]: Example injectors for working: Volcan, Bunni, Swift, Velocity, Seliware, Wave, Zenith.")

    pcall(function()
        local lp = game:GetService("Players").LocalPlayer
        if lp and lp.Kick then
            lp:Kick("[SkaneexxWare Announcement]: Bad injector detected: " .. injector)
        end
    end)
    
	wait(2)
    while true do
    end
else
    consolePrint("[SkaneexxWare Announcement]: Your injector supported.")
end

