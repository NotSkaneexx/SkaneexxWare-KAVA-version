local bypasstab = Window:CreateTab("🔓 AntiCheat bypass", nil) -- Title, Image
local MainSection = bypasstab:CreateSection("🔓 Main bypass")

  local Button = bypasstab:CreateButton({
    Name = "500 studs up",
    Callback = function()     
local player = game.Players.LocalPlayer

        if player then
            local character = player.Character or player.CharacterAdded:Wait()
            local rootPart = character:WaitForChild("HumanoidRootPart")
        
            local offsetY1 = 150
            local offsetY2 = 0
            local offsetX = 0
        
            local function TeleportWithOffset(offsetY)
                if rootPart then
                    local currentPosition = rootPart.Position
                    local newPosition = currentPosition + Vector3.new(offsetX, offsetY, 0)
                    rootPart.CFrame = CFrame.new(newPosition)
                else
                    print("HumanoidRootPart не найден!")
                end
            end
        
            TeleportWithOffset(offsetY1)
            TeleportWithOffset(offsetY2)
        
        else
            print("Игрок не найден!")
        end
        
        local trussHeight = 500
        local trussWidth = 2
        local trussDepth = 2
        local trussMaterial = Enum.Material.Metal
        local trussColor = Color3.fromRGB(100, 100, 100)
        
        local function CreateTruss()
            local player = game.Players.LocalPlayer
            if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local rootPart = player.Character:FindFirstChild("HumanoidRootPart")
                local head = player.Character:FindFirstChild("Head")
                if not head then
                    print("Head не найден у персонажа.")
                    return
                end
        
                local startPosition = rootPart.Position + head.CFrame.LookVector * 2
                local endPosition = startPosition + Vector3.new(0, trussHeight, 0)
        
                local truss = Instance.new("TrussPart")
                truss.Name = "GeneratedTruss"
                truss.Size = Vector3.new(trussWidth, trussHeight, trussDepth)
                truss.Material = trussMaterial
                truss.Color = trussColor
                truss.Anchored = true
                truss.CanCollide = true
                truss.CFrame = CFrame.new(startPosition, endPosition) * CFrame.Angles(math.rad(-90),0,0)
                truss.Parent = workspace
            else
                print("Персонаж не найден или HumanoidRootPart отсутствует.")
            end
        end
        
        CreateTruss()
    end,
 })

