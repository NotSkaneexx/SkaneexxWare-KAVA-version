local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local task = task

local Player = Players.LocalPlayer

local HEIGHT = 800
local WAIT_TIME = 0.8
local busy = false

local function doTeleport()
	if busy then return end
	busy = true

	local char = Player.Character or Player.CharacterAdded:Wait()
	if not char then
		warn("Character не найден.")
		busy = false
		return
	end

	local hrp = char:FindFirstChild("HumanoidRootPart") or char:WaitForChild("HumanoidRootPart", 2)
	if not hrp then
		warn("HumanoidRootPart не найден.")
		busy = false
		return
	end

	local originalCFrame = hrp.CFrame
	local targetCFrame = originalCFrame + Vector3.new(0, HEIGHT, 0)

	local ok, err = pcall(function()
		hrp.CFrame = targetCFrame
	end)
	if not ok then
		warn("Ошибка при телепорте вверх:", err)
		busy = false
		return
	end

	task.delay(WAIT_TIME, function()
		local curChar = Player.Character
		if not curChar then
			busy = false
			return
		end
		local curHrp = curChar:FindFirstChild("HumanoidRootPart") or curChar:WaitForChild("HumanoidRootPart", 2)
		if not curHrp then
			busy = false
			return
		end

		local ok2, err2 = pcall(function()
			curHrp.CFrame = originalCFrame
		end)
		if not ok2 then
			warn("Ошибка при возврате телепорта:", err2)
		end

		busy = false
	end)
end

