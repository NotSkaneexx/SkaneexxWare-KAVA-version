toggleCorner.Parent = toggleBtn

local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0, 20, 0, 40)
closeButton.Position = UDim2.new(1, -28, 0, 5)
closeButton.BackgroundTransparency = 1
closeButton.Text = "✖"
closeButton.TextColor3 = Color3.fromRGB(255, 0, 0)
closeButton.TextScaled = true
closeButton.Font = Enum.Font.GothamBlack
closeButton.Parent = mainFrame

local closeCorner = Instance.new("UICorner")
closeCorner.CornerRadius = UDim.new(1, 0)
closeCorner.Parent = closeButton

closeButton.MouseButton1Click:Connect(function()
	TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.In), {
		Size = UDim2.new(0, 0, 0, 0)
	}):Play()
	task.wait(0.3)
	screenGui:Destroy()
end)

local dragging = false
local dragInput, dragStart, startPos

mainFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = mainFrame.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

mainFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

game:GetService("UserInputService").InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

local ToggleKey = "x"
local DownEnabled = false
local StoredCameraCFrame = workspace.CurrentCamera.CFrame

local Camera = workspace.CurrentCamera

local function ToggleDown()
	DownEnabled = not DownEnabled
	toggleBtn.BackgroundColor3 = DownEnabled and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
	toggleBtn.Text = DownEnabled and "Disable 100 studs down" or "Enable 100 studs down"
	if DownEnabled then
		StoredCameraCFrame = Camera.CFrame
	end
end

toggleBtn.MouseButton1Click:Connect(ToggleDown)

Player:GetMouse().KeyDown:Connect(function(key)
	if key:lower() == ToggleKey:lower() then
		ToggleDown()
		if DownEnabled then
			StoredCameraCFrame = Camera.CFrame
		end
	end
end)

RunService.Heartbeat:Connect(function()
	local char = Player.Character
	if not char or not char:FindFirstChild("HumanoidRootPart") then return end
	local hrp = char.HumanoidRootPart

	if DownEnabled then
		local OriginalCFrame = hrp.CFrame
		hrp.CFrame = OriginalCFrame * CFrame.new(0, -100, 0)
		hrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
		RunService.RenderStepped:Wait()
		hrp.CFrame = OriginalCFrame

		Camera.CFrame = StoredCameraCFrame
	end
end)

Player.CharacterAdded:Connect(function(newChar)
	Player.Character:WaitForChild("HumanoidRootPart")
end)
    end,
 })


