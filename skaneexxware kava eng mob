local CoreGui = game:GetService("CoreGui")
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.IgnoreGuiInset = true
ScreenGui.ResetOnSpawn = false
ScreenGui.Name = "FadeTextGUI"
ScreenGui.Parent = CoreGui

local Background = Instance.new("Frame")
Background.BackgroundColor3 = Color3.new(0, 0, 0)
Background.BackgroundTransparency = 1
Background.Size = UDim2.new(1, 0, 1, 0)
Background.Position = UDim2.new(0, 0, 0, 0)
Background.Parent = ScreenGui

local ParticlesFrame = Instance.new("Frame")
ParticlesFrame.Size = UDim2.new(1, 0, 1, 0)
ParticlesFrame.Position = UDim2.new(0, 0, 0, 0)
ParticlesFrame.BackgroundTransparency = 1
ParticlesFrame.ZIndex = 0
ParticlesFrame.Parent = ScreenGui

local NUM_PARTICLES = 40
local particles = {}

for i = 1, NUM_PARTICLES do
	local particle = Instance.new("Frame")
	particle.Size = UDim2.new(0, 8, 0, 8)
	particle.AnchorPoint = Vector2.new(0.5, 0.5)
	particle.BackgroundColor3 = Color3.new(1, 1, 1)
	particle.BackgroundTransparency = 0.7 + math.random() * 0.3
	particle.ZIndex = 0

	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(1, 0)
	corner.Parent = particle

	local posX = math.random()
	local posY = math.random()
	particle.Position = UDim2.new(posX, 0, posY, 0)
	particle.Parent = ParticlesFrame

	local angle = math.random() * 2 * math.pi
	local speed = 0.002 + math.random() * 0.003

	table.insert(particles, {
		frame = particle,
		dx = math.cos(angle) * speed,
		dy = math.sin(angle) * speed,
	})
end

local particlesActive = true

task.spawn(function()
	while true do
		for i = #particles, 1, -1 do
			local p = particles[i]
			local pos = p.frame.Position
			local newX = pos.X.Scale + p.dx
			local newY = pos.Y.Scale + p.dy

			if newX > 1 or newX < 0 or newY > 1 or newY < 0 then
				if particlesActive then
					if newX > 1 then newX = 0 elseif newX < 0 then newX = 1 end
					if newY > 1 then newY = 0 elseif newY < 0 then newY = 1 end
					p.frame.Position = UDim2.new(newX, 0, newY, 0)
				else
					p.frame:Destroy()
					table.remove(particles, i)
				end
			else
				p.frame.Position = UDim2.new(newX, 0, newY, 0)
				local time = tick()
				p.frame.BackgroundTransparency = 0.5 + 0.4 * math.sin(time * 3 + i)
			end
		end

		if not particlesActive and #particles == 0 then
			break
		end

		task.wait(0.03)
	end
end)


local mainText = Instance.new("TextLabel")
mainText.Text = ""
mainText.Font = Enum.Font.GothamBold
mainText.TextSize = 150
mainText.Size = UDim2.new(1, 0, 1, 0)
mainText.Position = UDim2.new(0, 0, 0, 0)
mainText.BackgroundTransparency = 1
mainText.TextColor3 = Color3.new(1, 1, 1)
mainText.TextTransparency = 1
mainText.ZIndex = 1
mainText.Parent = ScreenGui

local leftImage = Instance.new("ImageLabel")
leftImage.Image = "rbxassetid://122902840554492"
leftImage.Size = UDim2.new(0, 150, 0, 150)
leftImage.Position = UDim2.new(0.5, -480, 0.5, -75)
leftImage.BackgroundTransparency = 1
leftImage.ImageTransparency = 1
leftImage.ZIndex = 1
leftImage.Parent = ScreenGui

local rightImage = Instance.new("ImageLabel")
rightImage.Image = "rbxassetid://122902840554492"
rightImage.Size = UDim2.new(0, 150, 0, 150)
rightImage.Position = UDim2.new(0.5, 330, 0.5, -75)
rightImage.BackgroundTransparency = 1
