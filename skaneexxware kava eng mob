local loadingText = Instance.new("TextLabel")
loadingText.Text = "Loading"
loadingText.Font = Enum.Font.Gotham
loadingText.TextSize = 40
loadingText.Size = UDim2.new(1, 0, 0, 60)
loadingText.Position = UDim2.new(0, 0, 0.8, 0)
loadingText.BackgroundTransparency = 1
loadingText.TextColor3 = Color3.new(1, 1, 1)
loadingText.TextTransparency = 1
loadingText.ZIndex = 1
loadingText.Parent = ScreenGui

for i = 1, 20 do
	local alpha = i * 0.05
	Background.BackgroundTransparency = 1 - alpha * 0.6
	loadingText.TextTransparency = 1 - alpha
	leftImage.ImageTransparency = 1 - alpha
	rightImage.ImageTransparency = 1 - alpha
	wait(0.03)
end

local fullText = "SkaneexxWare"
mainText.TextTransparency = 0
for i = 1, #fullText do
	mainText.Text = string.sub(fullText, 1, i)
	wait(0.06)
end

local animating = true
task.spawn(function()
	local state = 0
	local forward = true

	while animating do
		loadingText.Text = "Loading" .. string.rep(".", state)
		wait(0.3)

		if forward then
			state += 1
			if state > 3 then
				state = 2
				forward = false
			end
		else
			state -= 1
			if state < 0 then
				state = 1
				forward = true
			end
		end
	end
end)

wait(5)

animating = false
particlesActive = false

for i = 1, 20 do
	local alpha = i * 0.05
	Background.BackgroundTransparency = 0.4 + alpha * 0.6
	mainText.TextTransparency = alpha
	loadingText.TextTransparency = alpha
	leftImage.ImageTransparency = alpha
	rightImage.ImageTransparency = alpha
	wait(0.03)
end

ScreenGui:Destroy()

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local TextService = game:GetService("TextService")
local CoreGui = game:GetService("CoreGui")

if CoreGui:FindFirstChild("SkaneexxWatermark") then
    CoreGui:FindFirstChild("SkaneexxWatermark"):Destroy()
end

local settings = {
    Version = "V " .. version,
    Separator = " · ",
    Font = Enum.Font.GothamMedium,
    TextSize = 15,
    BackgroundColor = Color3.fromRGB(15, 15, 15),
    BackgroundTransparency = 0.1,
    TextColor = Color3.fromRGB(255, 255, 255),
    OutlineColor = Color3.fromRGB(20, 20, 20),
    CornerRadius = 20,
    Padding = 12,
    InitialPosition = UDim2.new(1, -1300, 0, -46)
}

local player = Players.LocalPlayer
local username = player.Name
local ping = "0 ping"
local time = ""

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "SkaneexxWatermark"
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = CoreGui

local Watermark = Instance.new("Frame")
Watermark.Name = "Watermark"
Watermark.BackgroundTransparency = 1
Watermark.Size = UDim2.new(0, 300, 0, 30 + settings.Padding * 2)
Watermark.Position = settings.InitialPosition
Watermark.AnchorPoint = Vector2.new(1, 0)
Watermark.Parent = ScreenGui

local Background = Instance.new("Frame")
Background.Name = "Background"
Background.Size = UDim2.new(1, 0, 0.8, 0)
Background.BackgroundColor3 = settings.BackgroundColor
Background.BackgroundTransparency = settings.BackgroundTransparency
Background.BorderSizePixel = 0
Background.Parent = Watermark

local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, settings.CornerRadius)
UICorner.Parent = Background

local TextContainer = Instance.new("Frame")
TextContainer.Name = "TextContainer"
TextContainer.BackgroundTransparency = 1
TextContainer.Size = UDim2.new(1, -settings.Padding * 2, 1, -settings.Padding * 2)
TextContainer.Position = UDim2.new(0, settings.Padding, 0, settings.Padding)
TextContainer.Parent = Background

local textParts = {}
local offsets = {
    Vector2.new(-1,-1), Vector2.new(0,-1), Vector2.new(1,-1),
    Vector2.new(-1,0), Vector2.new(1,0),
    Vector2.new(-1,1), Vector2.new(0,1), Vector2.new(1,1),
    Vector2.new(0,0)
}

for _, offset in pairs(offsets) do
    local textLabel = Instance.new("TextLabel")
    textLabel.Name = "TextPart"
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = ""
    textLabel.Font = settings.Font
    textLabel.TextSize = settings.TextSize
    textLabel.TextXAlignment = Enum.TextXAlignment.Left
    textLabel.TextYAlignment = Enum.TextYAlignment.Center
    textLabel.Position = UDim2.new(0, offset.X, 0, offset.Y)
    textLabel.TextColor3 = offset == Vector2.new(0,0) and settings.TextColor or settings.OutlineColor
    textLabel.ZIndex = 2
    textLabel.Parent = TextContainer
    table.insert(textParts, textLabel)
end

local function updateWatermark()
    local text = "SkaneexxWare"..settings.Separator..settings.Version..settings.Separator..
                username..settings.Separator..ping..settings.Separator..time
    
    for _, part in pairs(textParts) do
        part.Text = text
    end
    
    local textSize = TextService:GetTextSize(text, settings.TextSize, settings.Font, Vector2.new(9999, 30))
    Watermark.Size = UDim2.new(0, textSize.X + settings.Padding * 2, 0, 30 + settings.Padding * 2)
    TextContainer.Size = UDim2.new(1, -settings.Padding * 2, 1, -settings.Padding * 2)
    TextContainer.Position = UDim2.new(0, settings.Padding, 0, settings.Padding)
end

local function updateTime()
    time = os.date("%H:%M")
    updateWatermark()
end

local function updatePing()
    ping = math.floor(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue()).." ping"
    updateWatermark()
end

local dragging, dragStart, startPos

Watermark.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = Watermark.Position
    end
end)

Watermark.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        Watermark.Position = UDim2.new(
            startPos.X.Scale, 
            startPos.X.Offset + delta.X, 
            startPos.Y.Scale, 
            startPos.Y.Offset + delta.Y
        )
    end
end)

RunService.Heartbeat:Connect(function()
    updateTime()
    updatePing()
end)

updateWatermark()

