local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local teleportHeight = -499

pcall(function()
    if player.PlayerGui:FindFirstChild("RespawnGUI") then
        player.PlayerGui:FindFirstChild("RespawnGUI"):Destroy()
    end
end)

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "RespawnGUI"
screenGui.ResetOnSpawn = false
screenGui.IgnoreGuiInset = true
screenGui.Parent = player:WaitForChild("PlayerGui")

local mainFrame = Instance.new("Frame")
mainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
mainFrame.Size = UDim2.new(0, 230, 0, 110)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
mainFrame.BackgroundTransparency = 0.1
mainFrame.BorderSizePixel = 0
mainFrame.ClipsDescendants = true
mainFrame.Parent = screenGui

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 16)
corner.Parent = mainFrame

local glow = Instance.new("UIStroke")
glow.Thickness = 2
glow.Color = Color3.new(1, 1, 1)
glow.Transparency = 0.7
glow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
glow.Parent = mainFrame

local title = Instance.new("TextLabel")
title.AnchorPoint = Vector2.new(0.5, 0)
title.Position = UDim2.new(0.45, 0, 0, 12)
title.Size = UDim2.new(0.8, 0, 0, 20)
