local humanoid = character:FindFirstChild("Humanoid")

if humanoid then
  humanoid.Health = 0 
end
    end,
 })

 local MainSection = PlayerTab:CreateSection("🔝 Stats player")

local tpwalking = false
local tpWalkSpeed = 10

PlayerTab:CreateToggle({
    Name = "TPWalk",
    CurrentValue = false,
    Flag = "TPWalkToggle",
    Callback = function(Value)
        tpwalking = Value
        if not Value then return end

        local RunService = game:GetService("RunService")
        local hb = RunService.Heartbeat
        local chr = game.Players.LocalPlayer.Character
        local hum = chr and chr:FindFirstChildWhichIsA("Humanoid")

        if not hum then return end

        while tpwalking and chr and hum.Parent do
            local delta = hb:Wait()
            if hum.MoveDirection.Magnitude > 0 then
                local moveDirection = hum.MoveDirection
                chr:TranslateBy(moveDirection * delta * tpWalkSpeed)
            end
        end
    end
})

PlayerTab:CreateSlider({
    Name = "TPWalk speed",
    Range = {1, 1000},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 10,
    Flag = "TPWalkSpeedSlider",
    Callback = function(Value)
        tpWalkSpeed = Value
    end
})

 local GunsTab = Window:CreateTab("🔫 Guns", nil) -- Title, Image
 local MainSection = GunsTab:CreateSection("🏡 Main")
 
  local Button = GunsTab:CreateButton({
    Name = "No Recoil",
    Callback = function()     
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function applyAntiRecoil()
    if LocalPlayer.Character then
        for _, tool in ipairs(LocalPlayer.Character:GetChildren()) do
            if tool:IsA("Tool") and tool:FindFirstChild("ConfigGun") then
                local success, cfg = pcall(require, tool.ConfigGun)
                if success and type(cfg) == "table" then
                    cfg.RecoilX = 0
                    cfg.RecoilY = 0
                end
            end
        end
    end
end

while true do
    applyAntiRecoil()
    wait(5)
end
    end,
 })

  local Button = GunsTab:CreateButton({
    Name = "Legit AntiRecoil",
    Callback = function()     
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function applyAntiRecoil()
    if LocalPlayer.Character then
        for _, tool in ipairs(LocalPlayer.Character:GetChildren()) do
            if tool:IsA("Tool") and tool:FindFirstChild("ConfigGun") then
                local success, cfg = pcall(require, tool.ConfigGun)
                if success and type(cfg) == "table" then
                    cfg.RecoilX = 0.05
                    cfg.RecoilY = 0.08
                end
            end
        end
    end
end

while true do
    applyAntiRecoil()
    wait(5)
end
    end,
 })

  local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")

GunsTab:CreateToggle({
    Name = "Auto Reload",
    CurrentValue = false,
    Flag = "AutoReloadToggle",
    Callback = function(Value)
        _G.AutoReloadEnabled = Value
    end,
})

local function initAutoReload()
    local ammoLabel
    while not ammoLabel do
        if LocalPlayer.PlayerGui and LocalPlayer.PlayerGui:FindFirstChild("GunStat") then
            local gunStat = LocalPlayer.PlayerGui.GunStat
            if gunStat:FindFirstChild("Frame") then
                local frame = gunStat.Frame
                if frame:FindFirstChild("Ammo") then
                    ammoLabel = frame.Ammo
                end
            end
        end
        task.wait(0.5)
    end

    local reloadEvent = ReplicatedStorage:WaitForChild("ReplicationManager"):WaitForChild("Reload")

    local function checkAmmo()
        if not _G.AutoReloadEnabled then return end

        local text = ammoLabel.Text
        local currentAmmo = tonumber(text:match("^(%d+)/"))
        
        if currentAmmo and currentAmmo == 0 then
            reloadEvent:FireServer()
        end
    end

    ammoLabel:GetPropertyChangedSignal("Text"):Connect(checkAmmo)

    checkAmmo()
end

LocalPlayer.CharacterAdded:Connect(initAutoReload)
if LocalPlayer.Character then
    initAutoReload()
end

 local Button = GunsTab:CreateButton({
    Name = "AutoPistol",
    Callback = function()     
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function patchAutoMode()
    if LocalPlayer.Character then
        for _, tool in ipairs(LocalPlayer.Character:GetChildren()) do
            if tool:IsA("Tool") and tool:FindFirstChild("ConfigGun") then
                local success, cfg = pcall(require, tool.ConfigGun)
                if success and type(cfg) == "table" then
                    cfg.AutoMode = true
                end
            end
        end
    end
end

while true do
    patchAutoMode()
    wait(5)
end

    end,
 })

